{
  "name": "yikes-inc-starter-theme",
  "version": "6.1.1",
  "description": "LezWatch.TV Custom Theme (based on Yikes Inc. starter theme).",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "composer update && grunt build",
    "clean": "rm -rf node_modules vendor blocks/node_modules",
    "merge-to-develop": "currentBranch=$(git rev-parse --abbrev-ref HEAD) && git checkout development && git pull && git merge $currentBranch && git push origin development && git checkout $currentBranch",
    "updater": "npm update && composer update"
  },
  "author": "LezWatch.TV, Yikes Inc.",
  "license": "GPL-2.0",
  "repository": "https://github.com/lezwatch/lwtv-underscores/",
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "coffeescript": "^2.7.0"
  },
  "devDependencies": {
    "@lodder/grunt-postcss": "^3.1.1",
    "autoprefixer": "^10.4.15",
    "grunt": "^1.6.1",
    "grunt-auto-install": "^0.3.1",
    "grunt-contrib-cssmin": "^5.0.0",
    "grunt-contrib-sass": "^2.0.0",
    "grunt-contrib-uglify": "^5.2.2",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-terser": "^2.0.0",
    "postcss": "^8.4.29"
  }
}
